<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="xyz.lingview.dimstack.mapper.UploadMapper">
    <resultMap type="xyz.lingview.dimstack.domain.UploadArticle" id="UploadArticle">
        <result property="uuid" column="uuid" />
        <result property="article_id" column="article_id" />
        <result property="article_name" column="article_name" />
        <result property="article_path" column="article_path" />
        <result property="status" column="status" />
    </resultMap>

    <insert id="insertUploadArticle" parameterType="xyz.lingview.dimstack.domain.UploadArticle">
        INSERT INTO upload_article(uuid, article_id, article_name, article_path, status)
        VALUES (#{uuid}, #{article_id}, #{article_name}, #{article_path}, 1)
    </insert>
</mapper>
