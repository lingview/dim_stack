<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="xyz.lingview.dimstack.mapper.UploadMapper">
    <resultMap type="xyz.lingview.dimstack.domain.UploadArticle" id="UploadArticle">
        <result property="uuid" column="uuid" />
        <result property="article_id" column="article_id" />
        <result property="article_name" column="article_name" />
        <result property="article_cover" column="article_cover" />
        <result property="article_content" column="article_content" />
        <result property="tag" column="tag" />
        <result property="category" column="category" />
        <result property="alias" column="alias" />
        <result property="status" column="status" />
    </resultMap>

    <insert id="insertUploadArticle" parameterType="xyz.lingview.dimstack.domain.UploadArticle">
        INSERT INTO article(uuid, article_id, article_name, article_cover, article_content, tag, category, alias, status)
        VALUES (#{uuid}, #{article_id}, #{article_name}, #{article_cover}, #{article_content}, #{tag}, #{category}, #{alias}, #{status})
    </insert>
</mapper>
